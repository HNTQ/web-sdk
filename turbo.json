{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": []
    },
    "@authdog/authdog-react#build": {
      "dependsOn": ["^build", "@authdog/shared#build"],
      "outputs": ["dist/**", "build/**"]
    },
    "@authdog/shared#build": {
      "dependsOn": ["^build", "@authdog/types#build"],
      "outputs": ["dist/**", "build/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"]
    }
  }
}
