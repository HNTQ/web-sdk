{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": []
    },
    "@authdog/types#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "@authdog/shared#build": {
      "dependsOn": ["^build", "@authdog/types#build"],
      "outputs": ["dist/**", "build/**"]
    },
    "@authdog/browser#build": {
      "dependsOn": ["^build", "@authdog/shared#build"],
      "outputs": ["dist/**", "build/**"]
    },
    "@authdog/react#build": {
      "dependsOn": ["^build", "@authdog/browser#build"],
      "outputs": ["dist/**", "build/**"],
      "cache": true
    },
    "@authdog/nextjs#build": {
      "dependsOn": ["^build", "@authdog/react#build"],
      "outputs": ["dist/**", "build/**"]
    },
    "@playground/nextjs": {
      "dependsOn": ["^build", "@authdog/nextjs#build"]
    },
    "@playground/cra": {
      "dependsOn": ["^build", "@authdog/react#build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "ssr": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"],
      "cache": true
    }
  }
}
